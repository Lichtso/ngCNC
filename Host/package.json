{
  "name": "ngCNC",
  "version": "0.0.0-semantic-release",
  "description": "ngCNC web app host",
  "script": {
    "package": "npm-pkgbuild pkgbuild pacman makepkg"
  },
  "devDependencies": {
    "npm-pkgbuild": "^3.2.0"
  },
  "dependencies": {
    "gamepad": "^1.6.0",
    "gl-matrix": "^3.0.0"
  },
  "pacman": {
    "installdir": "/services/ngCNC",
    "install": "pacman/install.sh",
    "content": {
      "/usr/lib/tmpfiles.d/${name}.conf": "pacman/tmpfiles.conf"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lichtso/ngCNC",
    "directory": "Host"
  },
  "systemd": {
    "units": {
      "ngCNC": "systemd/ngCNC*"
    }
  }
}
